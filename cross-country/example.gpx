<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<gpx
  xmlns="http://www.topografix.com/GPX/1/1"
  creator="YOUR APPLICATION NAME HERE" version="1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:openrally="http://www.openrally.org/xmlschemas/GpxExtensions/v1.0.0"
  xsi:schemaLocation="http://www.topografix.com/GPX/1/1 http://www.topografix.com/GPX/1/1/gpx.xsd
                      http://www.openrally.org/xmlschemas/GpxExtensions/v1.0.0 openrally.xsd"
  >

  <metadata>
    <extensions>
      <openrally:units>metric</openrally:units>
      <openrally:distance>33.86</openrally:distance>
    </extensions>
  </metadata>

  <!-- minimum <wpt> must have a distance -->
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.0</openrally:distance>
    </extensions>
  </wpt>

  <!-- speed zones -->
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.1</openrally:distance>
      <openrally:dz/>
      <openrally:speed>65</openrally:speed>
    </extensions>
  </wpt>
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.2</openrally:distance>
      <openrally:fz/>
    </extensions>
  </wpt>

  <!-- danger !!! with WPS as per FIM -->
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.3</openrally:distance>
      <openrally:danger>3</openrally:danger>
      <openrally:wps open="800" clear="90"/>
    </extensions>
  </wpt>

  <!-- Stop for 3 seconds at road crossing -->
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.4</openrally:distance>
      <openrally:stop>3</openrally:stop>
    </extensions>
  </wpt>

  <!-- Neutralized fuel stop, 15 mins (900 seconds) -->
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.5</openrally:distance>
      <openrally:neutralization>900</openrally:neutralization>
      <openrally:fuel/>
    </extensions>
  </wpt>

  <!-- Tulip and Notes drawings -->
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.6</openrally:distance>
      <!-- PNG only -->
      <openrally:tulip><![CDATA[
          data:image/png;base64,
        ]]></openrally:tulip>
      <!-- PNG wrapped in SVG -->
      <openrally:notes><![CDATA[
        <svg>
          <img href="data:image/png;base64,"/>
        </svg>
        ]]></openrally:notes>
    </extensions>
  </wpt>

  <!-- Tulip and Notes drawings -->
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.6</openrally:distance>
      <!-- Full SVG -->
      <openrally:tulip><![CDATA[
        <svg>
          <circle cx="50" cy="50" r="40" stroke="black" stroke-width="3" fill="red" />
        </svg>
        ]]></openrally:tulip>
      <openrally:notes><![CDATA[
        <svg>
          <text>SVG Example</text>
        </svg>
        ]]></openrally:notes>
    </extensions>
  </wpt>

  <!-- or any other tags (using them all at once is nonsensical) -->
  <wpt lat="0" lon="0">
    <extensions>
      <openrally:distance>0.35</openrally:distance>
      <openrally:wpc clear="20" name="ABC"/>
      <openrally:wpm open="800" clear="200" name="XYZ"/>
      <openrally:wps open="800" clear="20" name="K99"/>
      <openrally:wpe clear="200"/>
      <openrally:dss/>
      <openrally:checkpoint/>
      <openrally:ass/>
      <openrally:timecontrol/>
      <openrally:timecontrol allowed="60"/>
      <openrally:cap>360</openrally:cap>
      <openrally:stop/> <!-- test with no time requirement -->
      <openrally:reset/> <!-- test with default to 0.00 -->
    </extensions>
  </wpt>
</gpx>
